{
    "name": "dainsys/locky",
    "description": "A laravel UI wrapper for spatie/laravel-permission with Bootstrap 4!",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Yismen Jorge",
            "email": "yismen.jorge@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": "^7.0",
        "laravel/ui": "^2.0",
        "dainsys/components": "*",
        "spatie/laravel-permission": "^3.0",
        "laravel/framework": "^7.0"
    },
    "require-dev": {
        "orchestra/testbench": "^5.2"
    },
    "autoload": {
        "psr-4": {
            "Dainsys\\Locky\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Dainsys\\Locky\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Dainsys\\Locky\\LockyServiceProvider"
            ]
        }
    }
}
